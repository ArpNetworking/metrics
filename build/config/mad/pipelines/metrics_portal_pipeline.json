{
  "name": "MetricsPortalPipeline",
  "timerStatistics": [
    "min",
    "median",
    "p90",
    "p95",
    "p99",
    "p99p9",
    "max",
    "count",
    "mean",
    "sum"
  ],
  "counterStatistics": [
    "min",
    "median",
    "p90",
    "p95",
    "p99",
    "p99p9",
    "max",
    "count",
    "mean",
    "sum"
  ],
  "gaugeStatistics": [
    "min",
    "median",
    "p90",
    "p95",
    "p99",
    "p99p9",
    "max",
    "count",
    "mean",
    "sum"
  ],
  "sources": [
    {
      "type": "com.arpnetworking.metrics.common.sources.FileSource",
      "name": "remet_gui_query_log",
      "sourceFile": "../metrics-portal/logs/metrics-portal-query.log",
      "stateFile": "../metrics-portal/logs/metrics-portal-query.log.state",
      "parser": {
        "type": "com.arpnetworking.metrics.mad.parsers.JsonToRecordParser",
        "defaultService": "metrics_portal",
        "defaultCluster": "metrics_dev",
        "defaultHost": "localhost.dev"
      }
    }
  ],
  "sinks": [
    {
      "type": "com.arpnetworking.tsdcore.sinks.PeriodicStatisticsSink",
      "name": "metrics_portal_periodic_statistics_sink"
    },
    {
      "type": "com.arpnetworking.tsdcore.sinks.PeriodFilteringSink",
      "name": "period_filtering_metrics_portal_telemetry_sink",
      "excludeGreaterThan": "PT1S",
      "sink": {
        "type": "com.arpnetworking.tsdcore.sinks.TimeThresholdSink",
        "name": "time_threshold_metrics_portal_telemetry_sink",
        "threshold": "PT10M",
        "sink": {
          "type": "com.arpnetworking.tsdcore.sinks.TelemetrySink",
          "name": "metrics_portal_telemetry_sink"
        }
      }
    },
    {
      "type": "com.arpnetworking.tsdcore.sinks.PeriodFilteringSink",
      "name": "period_filtering_metrics_portal_cluster_sink",
      "excludeLessThan": "PT1M",
      "sink": {
        "type": "com.arpnetworking.tsdcore.sinks.TimeThresholdSink",
        "name": "time_threshold_metrics_portal_cluster_sink",
        "threshold": "PT10M",
        "logOnly": true,
        "sink": {
          "type": "com.arpnetworking.tsdcore.sinks.AggregationServerSink",
          "name": "metrics_portal_cluster_sink",
          "serverAddress": "localhost",
          "serverPort": 7065
        }
      }
    }
  ]
}
