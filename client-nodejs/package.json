{
  "name": "tsd-metrics-client",
  "preferGlobal": false,
  "version": "0.2.8",
  "description": "Node JS class for publishing metrics as time series data (TSD).",
  "scripts": {
    "prepublish": "grunt",
    "test": "grunt cover --mochaReporter=xunit-file"
  },
  "main": "./lib/tsd-metrics-client",
  "repository": {
